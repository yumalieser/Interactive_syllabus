
@{
    ViewData["Title"] = "AddMultipleLesson";
    Layout = "~/Views/Shared/_AcademicianLayout.cshtml";
}

<div class="container">
    <br />
    <br />
    <br />
    <h3>Toplu Ders Ekleme</h3>
    <hr />
    <br />
    <br />
    <form asp-action="AddMultipleLesson" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label for="file">Excel Dosyası Yükle</label>
            <input type="file" class="form-control" name="file" />
        </div>
        <br />
        <br />
        <button type="submit" class="btn btn-primary">Dersleri Yükle</button>
    </form>
</div>

